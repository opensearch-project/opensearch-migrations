{
  "sourceClusters": {
    "source": {
      "version": "ES 7.10.2",
      "endpoint": "http://source:9200",
      "snapshotInfo": {
        "repos": {
          "my-repo": {
            "awsRegion": "us-east-1",
            "s3RepoPathUri": "s3://my-bucket/snapshots"
          }
        },
        "snapshots": {
          "snap1": {
            "repoName": "my-repo",
            "config": {
              "externallyManagedSnapshotName": "snap1"
            }
          }
        }
      }
    }
  },
  "targetClusters": {
    "target": {
      "endpoint": "http://target:9200"
    }
  },
  "snapshotMigrationConfigs": [
    {
      "fromSource": "source",
      "toTarget": "target",
      "perSnapshotConfig": {
        "snap1": [
          {
            "label": "docs",
            "documentBackfillConfig": {}
          }
        ]
      }
    }
  ]
}
