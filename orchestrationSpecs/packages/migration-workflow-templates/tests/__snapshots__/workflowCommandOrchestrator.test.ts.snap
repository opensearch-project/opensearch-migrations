STATUS_OUTPUT=$(workflow status --name "$WORKFLOW_NAME" 2>&1)
echo "Status: $STATUS_OUTPUT"

# Check if workflow is completed successfully
if echo "$STATUS_OUTPUT" | grep -q "Succeeded"; then
    echo "✅ Workflow completed successfully"
