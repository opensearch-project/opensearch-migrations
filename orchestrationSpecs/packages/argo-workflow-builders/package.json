{
  "name": "@opensearch-migrations/argo-workflow-builders",
  "version": "1.0.0",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./*": null,
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json && npm exec -y tsc-alias -- -p tsconfig.build.json",
    "type-check": "tsc -p tsconfig.build.json --noEmit",
    "clean": "tsc --build --clean",
    "make-templates": "tsx makeTemplates.cjs",
    "test": "jest --config ./jest.config.js"
  },
  "dependencies": {
    "@kubernetes/client-node": "^1.3.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}